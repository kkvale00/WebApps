@model _01___AziendaWeb.Models.Dipendente;<h2> Modifica Dipendente</h2><form action="/Dipendente/ModificaDipendente">    <input name="id" value="@Model.Id" readonly /> <br />    <input type="text" placeholder="Nome.." name="nome" value="@Model.Nome" /> <br>    <input type="text" placeholder="Cognome.." name="cognome" value="@Model.Cognome" /> <br>    <input type="text" placeholder="Reparto.." name="reparto" value="@Model.Reparto"/> <br>    <input type="text" placeholder="Residenza.." name="residenza" value="@Model.Residenza"/> <br>    Data di nascita <input type="date" name="dob" value="@Model.Dob.ToString("yyyy-MM-dd")"/> <br />    <input type="submit" value="Modifica" /></form>